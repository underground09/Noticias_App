<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="configuringnews.controller.Dialog1" xmlns="sap.m">
    <Dialog horizontalScrolling="true" showHeader="true" state="Warning" stretch="false" title="Change type" type="Standard" verticalScrolling="true" class="sapUiNoContentPadding">
        <content>
                <VBox justifyContent="Center" alignItems="Center" height="100%">
                <items>
                    <HBox justifyContent="Center" alignItems="Center">
                        <Text text="{i18n>popup_check}" class="sapUiMediumMargin"/>
                    </HBox>
                </items>
            </VBox>
        </content>
        <beginButton/>
        <buttons>
            <Button text="Accept" type="Accept" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onAcceptPressDialog" />
            <!--<Button text="Cancel" type="Reject" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onCancelPressDialog"/>-->
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>
