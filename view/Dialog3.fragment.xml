        <content>
            <TextArea id="IArea" value="" rows="4" placeholder="Enter the text" cols="200" maxLength="255" wrapping="None" enabled="true" required="false" valueStateText="Invalid entry" visible="true"/>
        </content>
        <beginButton/>
        <buttons>
            <Button text="Aceptar" type="Accept" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onAcceptPressDialog"/>
